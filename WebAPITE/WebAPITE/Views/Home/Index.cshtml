@model IEnumerable<WebAPITE.Models.About>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container "  style="margin-top:20px;">
    <div class="row" style="margin-bottom:20px;">
        <div class="col-5">
            <form action="@Url.Action("Index", "Home")">
                <input type="text" name="USERNAME" id="USERNAME" />
                <input type="submit" value="Search" />
            </form>
        </div>
    </div>
    <div class="row">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.USERNAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SYSTEM)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Port1433)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Port445)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Port3389)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.USB)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.USBPROTECT)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SYMANTECTUPDATE)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.VIRUS_PROTECTION)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.USERNAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.SYSTEM)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Port1433)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Port445)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Port3389)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.USB)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.USBPROTECT)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.SYMANTECTUPDATE)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.VIRUS_PROTECTION)
                    </td>
                   
                </tr>
            }

        </table>
    </div>
</div>

